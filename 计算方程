#include<stdio.h>
int main()
{
    int x,y;
    printf("请输入x：");
    scanf("%d",&x);
    if (x<0)
        y = 0;
        if (x>=0&&x<10)
            y = x;
        if (x>=10&&x<20)
            y = x+1;
        if (x>=20&&x<30)
            y = 2*x-1;
        if (x>=30)
            y = 3*x+1;
            printf("y:%d",y);
    return 0;
}
